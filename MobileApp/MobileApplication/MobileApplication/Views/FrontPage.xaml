<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileApplication.Views.FrontPage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand">
            <Image HorizontalOptions="CenterAndExpand" Source="http://prestonpeek.weebly.com/uploads/2/2/4/9/22497912/foodinmyfridge_orig.png"></Image>
            <Button Clicked="NaviagteToLoginPage" Text="Sign In"/>
            <Button Clicked="NaviagteToCreateAccountPage" Text="Create Account"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>